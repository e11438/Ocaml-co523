let identity x = x;;
(* function signature *)
val identity : 'a -> 'a = <fun> 

(**
method signature
**)

let t1=(1,"fale");;
val t1 : int * string = (1, "fale")    

let fst t =
let (a,b) =t in 
a;;
val fst : 'a * 'b -> 'a = <fun> 

let scnd t =
let (a,b) =t in 
b;;
val scnd : 'a * 'b -> 'b = <fun>     
